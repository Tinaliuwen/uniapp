.head, .gift {
  width: 100%;
  background: #FFFFFF;
}
 uni-tabbar {
  display: block !important;
}
.header {
  width: 100%;
  height: 270rpx;
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100% 100%;
}
.header-center {
  position: relative;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  left: 0;
  right: 0;
  width: 710rpx;
  height: 260rpx;
  margin: 0 auto;
  margin-top: -240rpx;
  border-radius: 10px;
}
.header-center .center-top {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  text-align: center;
  color: #FFFFFF;
}
.header-center .center-top text:first-child {
  font-size: 32rpx;
  margin-top: 48rpx;
}
.header-center .center-top text:last-child {
  font-size: 66rpx;
}
.header-center .center-bottom {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: end;
  -webkit-justify-content: flex-end;
          justify-content: flex-end;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  padding: 20rpx 0;
  border-top: 1px solid #f3f3f3;
}
.header-center .center-bottom text {
  font-size: 32rpx;
  font-weight: bold;
  color: #e98f3f;
}
.header-center .center-bottom view {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  width: 140rpx;
  height: 44rpx;
  margin: 0 22rpx 0 136rpx;
  border: 1px solid #ff594d;
  color: #d16f19;
  border-radius: 20rpx;
}
.header-center .center-bottom view text {
  font-size: 24rpx;
  font-weight: bold;
}
.header-center .center-bottom view image {
  width: 10rpx;
  height: 18rpx;
  margin-left: 4rpx;
}
.gift {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.gift .box {
  width: 50%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  padding: 30rpx 0;
}
.gift .box image {
  width: 60rpx;
  height: 60rpx;
  margin: 6rpx 0;
  margin-left: calc(50% - 140rpx);
}
.gift .box .right {
  width: 200rpx;
  color: #333333;
  font-size: 30rpx;
  font-weight: bold;
  margin-left: 20rpx;
}
.gift .box .right .bom {
  font-size: 26rpx;
  color: #999999;
}
.goods-section {
  background-color: #f3f3f3;
  padding: 0 20rpx 30rpx;
}
.goods-section .goods-header {
  color: #262626;
  font-size: 34rpx;
  font-weight: bold;
  margin: 20rpx 0;
}
.goods-section .goods-list {
  /* margin-bottom: 50rpx; */
}
.goods-section .goods-list .goods-item {
  width: 345rpx;
  background-color: #ffffff;
  margin-bottom: 20rpx;
  border-radius: 10rpx;
}
.goods-section .goods-list .goods-item image {
  width: 100%;
  height: 330rpx;
  border-radius: 10rpx 10rpx 0rpx 0rpx;
}
.goods-section .goods-list .goods-item .title {
  padding: 10rpx 30rpx 0;
  font-size: 26rpx;
  color: #333;
  font-weight: bold;
  line-height: 1.4;
}
.goods-section .goods-list .goods-item .price {
  padding: 14rpx 30rpx;
  font-size: 36rpx;
  font-weight: bold;
  color: #ff594d;
}
.goods-section .goods-list .goods-item .price text {
  font-size: 20rpx;
  color: #999;
  text-decoration: line-through;
  margin-left: 15rpx;
}

