<view class="order data-v-f0a286b2"><swiper class="swiper-box data-v-f0a286b2" current="{{tabIndex}}" duration="300" data-event-opts="{{[['change',[['changeTab',['$event']]]]]}}" bindchange="__e"><swiper-item class="tab-content data-v-f0a286b2"><scroll-view class="list-scroll-content data-v-f0a286b2" scroll-y="{{true}}" data-event-opts="{{[['scrolltolower',[['scrolltolower',['$event']]]]]}}" bindscrolltolower="__e"><block wx:for="{{listData}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="order-item data-v-f0a286b2"><view class="order-top data-v-f0a286b2"><view class="num data-v-f0a286b2">{{"订单单号："+item.order_no}}</view><block wx:if="{{item.status==1}}"><view class="state data-v-f0a286b2">待付款</view></block><block wx:if="{{item.status==2}}"><view class="state data-v-f0a286b2">已完成</view></block></view><block wx:for="{{item.allGoods}}" wx:for-item="it" wx:for-index="idx" wx:key="idx"><view data-event-opts="{{[['tap',[['jump',['/pages/newPeople/newdetails?orderid='+item.id+'&status='+item.status,'$0'],[[['listData','',index,'id']]]]]]]}}" class="order-center data-v-f0a286b2" bindtap="__e"><view class="img data-v-f0a286b2"><image class="order-img data-v-f0a286b2" src="{{it.logo}}" mode></image></view><view class="center-right data-v-f0a286b2"><view class="title data-v-f0a286b2">{{it.title}}</view><view class="detail data-v-f0a286b2"><view class="detail-price data-v-f0a286b2">{{"￥"+item.pay_price}}</view><view class="detail-num data-v-f0a286b2">x1</view></view></view></view></block><view class="total data-v-f0a286b2">共1件，合计：<view class="money data-v-f0a286b2">{{"￥"+item.pay_price}}</view></view><block wx:if="{{item.status==0}}"><view class="bottom data-v-f0a286b2"><view data-event-opts="{{[['tap',[['handleRemoveOrder',['$0'],[[['listData','',index,'id']]]]]]]}}" class="warn data-v-f0a286b2" bindtap="__e">删除订单</view></view></block><block wx:if="{{item.status==2}}"><view class="bottom data-v-f0a286b2"><view data-event-opts="{{[['tap',[['cancel',['$0'],